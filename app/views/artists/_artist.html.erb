<div id="<%= dom_id artist %>">
  <p>
    <strong>Nickname:</strong>
    <%= artist.nickname %>
  </p>

  <p>
    <strong>First name:</strong>
    <%= artist.first_name %>
  </p>

  <p>
    <strong>Last name:</strong>
    <%= artist.last_name %>
  </p>

  <p>
    <strong>Birth date:</strong>
    <%= artist.birth_date %>
  </p>

  <p>
    <strong>Id state:</strong>
    <%= artist.state_id %>
  </p>

  <p>
    <strong>Bio:</strong>
    <%= artist.bio %>
  </p>

</div>
