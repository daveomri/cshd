<div id="root">
  <%= render partial: "static_pages/header" %>

  <div>
    <p style="color: green"><%= notice %></p>
    <% if defined?(current_user) && current_user %>
      <h1>Hello <%= current_user.username%></h1>
      <h3>Your role is <%= current_user.role_id%></h3>
      <p><a href="<%= artists_path%>">Artists</a></p>
      <p><a href="<%= songs_path%>">Songs</a></p>
      <p><a href="<%= users_path%>">Users</a></p>
      <p><a href="<%= reviews_path%>">Reviews</a></p>
      -
      <p><a href="<%= states_path%>">States</a></p>
      <p><a href="<%= roles_path%>">Roles</a></p>
      <p><a href="<%= languages_path%>">Languages</a></p>
      <p><a href="<%= interactions_path%>">Interactions</a></p>
    <% else %>
      <h1>Hello stranger, consider <a href="<%= new_user_path%>">registration</a>.</h1>
      <p><a href="<%= artists_path%>">Artists</a></p>
      <p><a href="<%= songs_path%>">Songs</a></p>
      <p><a href="<%= users_path%>">Users</a></p>
      <p><a href="<%= reviews_path%>">Reviews</a></p>
    <% end %>
  </div>
</div>